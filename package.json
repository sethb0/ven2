{
  "name": "@ven2/ven2",
  "version": "0.4.1",
  "description": "Command-line tools for Exalted 2.5e character management",
  "author": "Seth Blumberg <sethb@pobox.com>",
  "license": "MIT",
  "scripts": {
    "posttest": "eslint .",
    "prepare": "rm -rf dist && babel src --out-dir dist --copy-files",
    "start": "node --require @babel/register src",
    "test": "mocha",
    "web": "rm -rf web/schema && mkdir -p web/schema && (for x in src/schemata/*.yml; do node utils/y2j.js \"$x\" web/schema/$(basename \"$x\" .yml).json; done) && firebase deploy --only hosting"
  },
  "bin": {
    "venator": "./bin/venator.js"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    "Anathema",
    "Exalted",
    "CLI",
    "YAML"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=6.4.1"
  },
  "os": [
    "!win32"
  ],
  "repository": "github:sethb0/ven2",
  "homepage": "https://github.com/sethb0/ven2#readme",
  "bugs": {
    "url": "https://github.com/sethb0/ven2/issues"
  },
  "dependencies": {
    "@sethb0/yaml-utils": "^0.6.0",
    "ajv": "^6.5.4",
    "ajv-merge-patch": "^4.1.0",
    "concat-stream": "^1.6.2",
    "js-yaml": "^3.12.0",
    "object.fromentries": "^2.0.0",
    "pdfmake": "^0.1.38",
    "pump": "^3.0.0",
    "xmldom": "^0.1.27",
    "xmltojson": "^1.3.5",
    "yargs": "^12.0.2"
  },
  "peerDependencies": {
    "@ven2/data": "*",
    "@ven2/images": "0.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@sethb0/eslint-config-mfllc": "^0.6.1",
    "@sethb0/mockstd": "^0.1.0",
    "@ven2/data": "*",
    "@ven2/images": "0.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-babel": "^5.2.1",
    "mocha": "^5.2.0"
  }
}
