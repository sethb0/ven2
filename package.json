{
  "name": "@ven2/ven2",
  "version": "0.5.4",
  "description": "Command-line tools for Exalted 2.5e character management",
  "author": "Seth Blumberg <sethb@pobox.com>",
  "license": "MIT",
  "scripts": {
    "posttest": "eslint .",
    "prepare": "rm -rf dist && babel src --out-dir dist --copy-files",
    "start": "node --require @babel/register src",
    "test": "mocha",
    "web": "./utils/web-deploy.sh"
  },
  "bin": {
    "venator": "./bin/venator.js"
  },
  "files": [
    "/bin/**/*.js",
    "/npm-shrinkwrap.json",
    "/dist/**/*.js",
    "/dist/**/*.yml"
  ],
  "main": "dist/index.js",
  "keywords": [
    "Anathema",
    "Exalted",
    "CLI",
    "YAML"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6.11.3"
  },
  "os": [
    "!win32"
  ],
  "repository": "github:sethb0/ven2",
  "homepage": "https://github.com/sethb0/ven2#readme",
  "bugs": {
    "url": "https://github.com/sethb0/ven2/issues"
  },
  "dependencies": {
    "@sethb0/yaml-utils": "^0.6.2",
    "ajv": "^6.10.2",
    "ajv-merge-patch": "^4.1.0",
    "concat-stream": "^1.6.2",
    "js-yaml": "^3.13.1",
    "mongodb": "^3.3.2",
    "pdfmake": "^0.1.60",
    "pump": "^3.0.0",
    "xmldom": "^0.1.27",
    "xmltojson": "^1.3.5",
    "yargs": "^14.0.0"
  },
  "peerDependencies": {
    "@ven2/data": "*",
    "@ven2/images": "0.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@sethb0/eslint-config-mfllc": "^0.7.1",
    "@sethb0/mockstd": "^0.1.0",
    "@ven2/data": "*",
    "@ven2/images": "0.0.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "core-js": "^3.2.1",
    "eslint": "^6.5.1",
    "eslint-plugin-babel": "^5.3.0",
    "mocha": "^10.2.0"
  }
}
